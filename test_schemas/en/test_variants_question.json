                            "question_variants": [
                                {
                                    "question": {
                                        "id": "age-confirmation-question",
                                        "type": "General",
                                        "title": "You are over 16?",
                                        "answers": [
                                            {
                                                "id": "age-confirm-answer",
                                                "type": "Radio",
                                                "mandatory": true,
                                                "options": [
                                                    {
                                                        "label": "Yes",
                                                        "value": "Yes"
                                                    },
                                                    {
                                                        "label": "No",
                                                        "value": "No"
                                                    }
                                                ]
                                            }
                                        ]
                                    },
                                    "when": [
                                        {
                                            "id": "age-answer",
                                            "condition": "greater than",
                                            "value": 16
                                        },
                                        {
                                            "id": "proxy-answer",
                                            "condition": "not equals",
                                            "value": "No, I am answering on their behalf"
                                        }
                                    ]
                                },
                                {
