{
    "mime_type": "application/json/ons/eq",
    "language": "en",
    "schema_version": "0.0.1",
    "data_version": "0.0.3",
    "survey_id": "0",
    "title": "Question Title Test",
    "theme": "default",
    "description": "A questionnaire to test question titles",
    "metadata": [
        {
            "name": "user_id",
            "type": "string"
        },
        {
            "name": "period_id",
            "type": "string"
        },
        {
            "name": "ru_name",
            "type": "string"
        }
    ],
    "questionnaire_flow": {
        "type": "Linear",
        "options": { "include_summary": true }
    },
    "sections": [
        {
            "id": "default-section",
            "groups": [
                {
                    "id": "group",
                    "blocks": [
                        {
                            "type": "Question",
                            "id": "single-title-block",
                            "question": {
                                "id": "single-title-question",
                                "title": "How are you feeling??",
                                "type": "General",
                                "answers": [
                                    {
                                        "type": "Radio",
                                        "id": "feeling-answer",
                                        "mandatory": true,
                                        "options": [
                                            {
                                                "label": "Good",
                                                "value": "Good"
                                            },
                                            {
                                                "label": "Bad",
                                                "value": "Bad"
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
